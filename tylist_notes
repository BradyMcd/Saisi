TY_LIST_CONCORD( TY, CMP )
  CMP(a,b) returns less than 0 if a < b, greater than 0 if a > b and 0 if a == b
  TY_concord_add( list, pl )
  TY_find( list, pl )

ITER_IFACE( TY )
  iter_next_TY( list )
  iter_peek_TY( list )
  iter_insert_TY( list, pl )

TY_LIST_IFACE( TY, DROP )
  TY_list
  new_TY_list( )
  new_TY_list_sized( cap )
  TY_set( list, pl, idx )
  TY_insert( list, pl, idx )
  TY_add( list, pl )
  TY_get( list, idx )
  TY_remove( list, idx )
  TY_pop( list )
